{
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": [],
        "includePattern": ".+\\.js(x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "node_modules/docdash", 
        "destination": "", 
        "recurse": true
    },
    "plugins": [],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    }
}
